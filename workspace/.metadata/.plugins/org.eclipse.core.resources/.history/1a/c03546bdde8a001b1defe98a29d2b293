<%@page import="java.util.LinkedHashMap"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<%
	Map<String , String> bloodMap = new LinkedHashMap<>();
	bloodMap.put("a","");
	bloodMap.put("b","");
	bloodMap.put("ab","");
	bloodMap.put("o","");
%>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
<select>
	<%
		for(Entry : bloodMap.entrySet())
	%>
</select>
</body>
</html>