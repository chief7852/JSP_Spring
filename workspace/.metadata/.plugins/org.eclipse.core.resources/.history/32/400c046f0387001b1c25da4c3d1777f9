package kr.or.ddit.enumpkg;

public enum MimeType {
	JSON("application/json;charset=UTF-8"),XML("application/json;charset=UTF-8")
	,PLAIN("application/plain;charset=UTF-8"),HTML("application/json;charset=UTF-8");
	
	
	
	private MimeType(String mime) {
		this.mime = mime;
	}
	private String mime;
	public String getMime() {
		return mime;
	}
	
}
