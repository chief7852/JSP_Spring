package kr.or.ddit.enumpkg;

public enum BrowserType2{
	WIN("엣지"), MAC("안드로이드"), ANDROID("안드로이드"),IPHONE("아이폰"), OTHER("기타등등");
	BrowserType2(String browserName){
		this.browserName = browserName;
	}
	private String browserName;
	public String getBrowserName(){
		return this.browserName;
	}
	
	
	public static String getBrowserName(String agent) {
		BrowserType2[] types =BrowserType2.values();
		agent = agent.toUpperCase(); 
		BrowserType2 searched = OTHER;
		for(BrowserType2 tmp : types){
			if(agent.contains(tmp.name())){
				searched = tmp;
				break;
			}
		}
		String name = searched.getBrowserName();
		return name;
	}
}